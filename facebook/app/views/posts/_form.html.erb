<%= form_for @post do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <%= f.label :title %>
  <%= f.text_field :title, size: "10x1", class: "form-control" %>

  <%= f.label :content %>
  <%= f.text_area :content, size: "10x5", class: "form-control" %>

  <%= f.submit "Create Post" %>
<% end %>
